{
  "name": "igorzoriy.com",
  "version": "0.1.0",
  "description": "IgorZoriy.com homepage",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/igorzoriy/igorzoriy.com.git"
  },
  "author": "Igor Zoriy",
  "bugs": {
    "url": "https://github.com/igorzoriy/igorzoriy.com/issues"
  },
  "homepage": "http://igorzoriy.com",
  "scripts": {
    "resume": "resume export -t spartan resume.pdf",
    "serve": "npm run resume && parcel ./index.pug",
    "build": "npm run resume && parcel build --public-url ./ ./index.pug",
    "deploy": "npm run build && cp CNAME dist/CNAME && gh-pages -d dist"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "jsonresume-theme-spartan": "^0.2.1",
    "pug": "^2.0.3",
    "resume-cli": "^0.4.19",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "parcel-bundler": "^1.11.0"
  }
}
